<%= render :partial => 'shared/nav' %>
<div id='gk-show-lot-page' class='gk-page'>
  <h1><%= @lot_presenter.lot_type %> Plate</h1>
  <div class='col-md-6'>
    <%= render :partial => 'summary', :layout => :'layouts/section', :locals => {:title => "Summary", :lot=>@lot_presenter} %>
  </div><div class='col-md-6'>
    <%= render :partial => 'create_stamp_plates', :layout => :'layouts/section', :locals => {:title => "Create Stamp Plates", :lot=>@lot_presenter} %>
  </div><div class='col-md-12'>
    <%= render :partial => 'plates', :layout => :'layouts/section', :locals => {:title => "Plates", :lot => @lot_presenter} %>
  </div>
</div>
<div class='hidden'>
  <div class="alert alert-danger alert-dismissable unknown-error"><button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>An unknown error has occured! Please contact support.</div>
</div>
